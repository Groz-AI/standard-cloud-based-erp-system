{
  "name": "@retail-erp/backend",
  "version": "1.0.0",
  "description": "Retail ERP Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node server.js",
    "dev:ts": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "db:migrate": "node-pg-migrate up",
    "db:migrate:down": "node-pg-migrate down",
    "db:seed": "tsx src/database/seed.ts",
    "db:reset": "npm run db:migrate:down -- --count 999 && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jose": "^5.2.0",
    "node-pg-migrate": "^6.2.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  }
}
